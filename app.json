{
  "expo": {
    "name": "SkillsUpriseApp",
    "slug": "skillsuprise-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.skillsuprise.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs location data to provide personalized content.",
        "NSContactsUsageDescription": "This app needs access to contacts to sync user information.",
        "NSCameraUsageDescription": "This app needs camera access for scanning features."
      }
    },
    "android": {
      "package": "com.skillsuprise.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_CONTACTS",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "firebaseConfig": {
        "apiKey": "your api key",
        "authDomain": "auth domain",
        "databaseURL": "database",
        "projectId": "project id",
        "storageBucket": "storage bucket",
        "messagingSenderId": "sender id",
        "appId": "app id"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
